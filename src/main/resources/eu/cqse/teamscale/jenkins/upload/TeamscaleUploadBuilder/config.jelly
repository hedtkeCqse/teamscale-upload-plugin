<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form" xmlns:pbs="/lib/postbuildscript" xmlns:c="/lib/credentials">
    <f:entry title="${%URL}" field="url">
        <f:textbox />
    </f:entry>
    <f:entry field="credentialsId" title="${%Credentials}">
        <c:select/>
    </f:entry>
    <f:entry title="${%TeamscaleProject}" field="teamscaleProject">
        <f:textbox />
    </f:entry>
    <f:entry title="${%Partition}" field="partition">
        <f:textbox />
    </f:entry>
    <f:entry title="${%Repository}" field="repository">
        <f:textbox />
    </f:entry>
    <f:entry title="${%UploadMessage}" field="uploadMessage">
        <f:textbox />
    </f:entry>
    <f:entry title="${%IncludePattern}" field="includePattern">
            <f:textbox />
    </f:entry>
    <f:entry title="${%ReportFormatID}" field="reportFormatId">
            <f:textbox />
    </f:entry>
    <f:entry title="${%Revision}" field="revision">
                <f:textbox />
        </f:entry>

</j:jelly>
